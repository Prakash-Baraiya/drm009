{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "manifest_version": 2,
   "name": "Learnyst Capture",
   "browser_action": {"default_icon": "icon128.png"},
   "permissions": [ "activeTab", "webRequest", "webRequestBlocking", "*://*/*", "tabs", "clipboardWrite", "storage" ],
   "version": "1.0",
   "web_accessible_resources": [
	"background.js",
	"script.js",
    "popup.html",
    "background.gif"
  ]
}